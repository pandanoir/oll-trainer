{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": [
      "esnext",
      "esnext.asynciterable",
      "dom"
    ],
    "esModuleInterop": true,
    "sourceMap": true,
    "strict": true,
    "baseUrl": ".",
    "types": [
      "node",
      "jest",
      "vite/client"
    ],
    "noEmit": true,
    "jsx": "react-jsx",
  },
  "include": [
    "./src",
    "./src/types"
  ]
}